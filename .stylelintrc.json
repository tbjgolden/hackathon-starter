{
  "extends": ["stylelint-config-standard-scss", "stylelint-config-prettier"],
  "rules": {
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": [
          "/colour/",
          "/behaviour/",
          "/maths/",
          "/mitrelimit/",
          "/centre/",
          "/capitalise/",
          "/grey/"
        ]
      }
    ],
    "custom-property-pattern": null
  }
}
