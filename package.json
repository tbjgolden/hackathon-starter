{
  "name": "hackathon-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "help": "node ./.scripts/help.mjs",
    "help:dev": "echo 'Start development environment'",
    "dev": "concurrently --raw \"vite\" \"PORT=3001 keystone dev\"",
    "help:prod": "echo 'Start production environment'",
    "prod": "NODE_ENV=production concurrently --kill-others-on-fail \"npm run types\" \"npm run lint\" \"vite build\" \"keystone build\" && concurrently -k \"node ./.scripts/prod-client.mjs\" \"PORT=3001 keystone start\"",
    "help:md-to-tsx": "echo 'Generate a page from all markdown files (src/pages/**/*.md)'",
    "md-to-tsx": "node ./.scripts/md-to-tsx.mjs",
    "help:lint": "echo 'Lint all files, output to stdout'",
    "lint": "eslint .",
    "help:types": "echo 'Typecheck all TS files, output to stdout'",
    "types": "tsc && echo 'Type-checking passed'",
    "help:prepare": "echo 'Install (or reinstall) git hooks'",
    "prepare": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks"
  },
  "dependencies": {
    "@apollo/client": "^3.6.0",
    "@keystone-6/auth": "^2.0.0",
    "@keystone-6/core": "^1.1.0",
    "@keystone-6/fields-document": "^2.0.0",
    "@nivo/core": "^0.79.0",
    "@nivo/geo": "^0.79.1",
    "@types/dedent": "^0.7.0",
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "baseui": "^11.0.1",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "concurrently": "^7.1.0",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.18.0",
    "graphql": "^15.8.0",
    "helmet": "^5.0.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "mdast-util-from-markdown": "^1.2.0",
    "morgan": "^1.10.0",
    "postcss-nested": "^5.0.6",
    "prettier": "2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "sass": "^1.49.11",
    "styletron-engine-atomic": "^1.4.8",
    "styletron-engine-monolithic": "^0.0.6",
    "styletron-react": "^6.0.2",
    "typescript": "^4.5.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pages": "^0.22.0",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/htmltojsx": "^0.0.30",
    "@types/js-yaml": "^4.0.5",
    "@types/prettier": "^2.4.4",
    "@types/styletron-engine-atomic": "^1.1.1",
    "@types/styletron-engine-monolithic": "^0.0.0",
    "@types/styletron-react": "^5.0.3",
    "eslint": "^8.12.0",
    "eslint-plugin-baseui": "^11.0.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "htmltojsx": "^0.2.5",
    "js-yaml": "^4.1.0",
    "mdast-util-frontmatter": "^1.0.0",
    "micromark-extension-frontmatter": "^1.0.0",
    "prettier-eslint": "^13.0.0",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "vite": "^2.9.5"
  }
}
